{
  "name": "openmrs-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "tasks.max": "1",
    "database.hostname": "mysql",
    "database.port": "3306",
    "database.user": "root",
    "database.password": "openmrs",
    "database.server.id": "184054",
    "database.server.name": "dbserver1",
    "topic.prefix": "dbserver1",
    "database.include.list": "openmrs",
    "table.include.list": "openmrs.patient,openmrs.person_name",
    "database.history.kafka.bootstrap.servers": "kafka-broker:9092",
    "database.history.kafka.topic": "schema-changes.openmrs",
    "include.schema.changes": "true",
    "snapshot.mode": "initial",
    "snapshot.locking.mode": "none",
    "database.jdbc.driver": "com.mysql.cj.jdbc.Driver",
    "database.ssl.mode": "disabled",
    "database.connectionTimeZone": "SERVER",
    "database.allowPublicKeyRetrieval": "true"
  }
}
